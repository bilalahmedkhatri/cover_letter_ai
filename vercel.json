{
  "rewrites": [
    {
      "source": "/:path*",
      "missing": [
        {
          "type": "file",
          "value": ":path*"
        }
      ],
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ]
}